<?xml version="1.0" encoding="utf-8"?>
<r:RDF xmlns="http://www.mozilla.org/2004/em-rdf#"
       xmlns:r="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <r:Description about="urn:mozilla:install-manifest">
    <creator>Erik Vold</creator>
    <id>restartless.restart@erikvold.com</id>
    <name>Restartless Restart</name>
    <version>7pre</version>
    <bootstrap>true</bootstrap>
    <type>2</type>
    <homepageURL>http://github.com/erikvold/restartless-restart-ffext</homepageURL>
    <description>Fennec users enable to restart, otherwise a File -> Restart menu item will be added.</description>

    <contributor>Greg Parris; http://phob.net/</contributor>
    <contributor>Nils Maier; https://tn123.org/</contributor>

    <!-- Firefox -->
    <targetApplication>
      <r:Description>
        <id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</id>
        <minVersion>4.0b4</minVersion>
        <maxVersion>6.*</maxVersion>
      </r:Description>
    </targetApplication>
    <!-- SeaMonkey -->
    <targetApplication>
      <r:Description>
        <id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</id>
        <minVersion>2.1a3</minVersion>
        <maxVersion>2.1.*</maxVersion>
      </r:Description>
    </targetApplication>
    <!-- Thunderbird -->
    <targetApplication>
      <r:Description>
        <id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</id>
        <minVersion>3.3a1</minVersion>
        <maxVersion>3.*</maxVersion>
      </r:Description>
    </targetApplication>
    <!-- Fennec -->
    <targetApplication>
      <r:Description>
        <id>{a23983c0-fd0e-11dc-95ff-0800200c9a66}</id>
        <minVersion>4.0b4</minVersion>
        <maxVersion>4.*</maxVersion>
      </r:Description>
    </targetApplication>
  </r:Description>
</r:RDF>
